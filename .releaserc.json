        "@semantic-release/changelog",
        [
            "semantic-release-vsce",
            {
                "packageVsix": "datapack-language-server.vsix"
            }
        ],
        [
            "@semantic-release/github",
            {
                "assets": "datapack-language-server-*.vsix",
                "releasedLabels": [":rocket: released"]
            }
        ],
        [
            "@semantic-release/git",
            {
                "message": "ðŸ”– v${nextRelease.version}\n\n[skip ci]\n\nhttps://github.com/SPGoding/vscode-datapack-helper-plus/releases/tag/${nextRelease.gitTag}"
            }
        ]
    ],
    "branches": "released"
}
